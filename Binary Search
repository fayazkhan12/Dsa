// Binary search 
#include<iostream>
using namespace std;

int main(){
int key =8;
int arr[]={1,3,5,6,7,8,9};
    
    int n=sizeof(arr)/sizeof(arr[0]);
//Performing binary search 
int low =arr[0];
int high= n;
int mid=high + low /2;
for(int i=0;i<=n;i++)
{
    while(key>arr[i])
    {
        mid++;
    }
     if(key=arr[i])
    {
        cout<<"element found "<<key<<endl;
        
    }
    else{
        cout<<"element not found "<<endl;
    }
}

    


    
}
